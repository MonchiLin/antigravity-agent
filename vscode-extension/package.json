{
    "name": "antigravity-vscode",
    "displayName": "Antigravity Agent",
    "description": "Manage your Antigravity accounts directly from VS Code",
    "version": "0.0.1",
    "type": "module",
    "engines": {
        "vscode": "^1.90.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./dist/extension.js",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "antigravity-sidebar",
                    "title": "Antigravity",
                    "icon": "resources/antigravity-icon.svg"
                }
            ]
        },
        "views": {
            "antigravity-sidebar": [
                {
                    "type": "webview",
                    "id": "antigravity-view",
                    "name": "Accounts"
                }
            ]
        },
        "commands": [
            {
                "command": "antigravity.refresh",
                "title": "Refresh Accounts",
                "icon": {
                    "light": "resources/refresh-light.svg",
                    "dark": "resources/refresh-dark.svg"
                }
            }
        ],
        "menus": {
            "view/title": [
                {
                    "command": "antigravity.refresh",
                    "when": "view == antigravity-view",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run package",
        "compile": "vite build",
        "watch": "vite build --watch",
        "package": "vite build",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@tomjs/vite-plugin-vscode": "^5.2.0",
        "@types/glob": "^8.1.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "20.2.5",
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.0",
        "@types/vscode": "^1.90.0",
        "@typescript-eslint/eslint-plugin": "^5.59.8",
        "@typescript-eslint/parser": "^5.59.8",
        "@vitejs/plugin-react": "^4.0.0",
        "@vscode/test-electron": "^2.3.2",
        "eslint": "^8.41.0",
        "glob": "^8.1.0",
        "mocha": "^10.2.0",
        "typescript": "^5.1.3",
        "vite": "^4.4.0"
    },
    "dependencies": {
        "@vscode/webview-ui-toolkit": "^1.4.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    }
}